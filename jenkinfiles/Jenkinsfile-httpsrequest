pipeline {
  agent any
  stages {
    stage ('Build') {
      environment {
        // API URL
        API_URL = "https://reqres.in/api/users"
      }
      steps {
        script {
          def response = httpRequest env.API_URL
          println("Status: "+response.status)
          println("Content: "+response.content)
        }
      }
    }
  }
}
